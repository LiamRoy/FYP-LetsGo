<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fyp_staycation" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FYPStayCation" >
        <activity android:name=".FilterActivity" />
        <activity android:name=".ParticipantsActivity" />
        <activity android:name=".TripsActivity" />
        <activity android:name=".CreateTripActivity" />
        <activity android:name=".ChatActivity" />
        <activity android:name=".GroupChatActivity" />
        <activity android:name=".LocationDetailsActivity" />
        <activity android:name=".AdminPickActivity" />
        <activity android:name=".AdminActivity" />
        <activity android:name=".AdminLogin"
            android:theme="@style/Theme.FYPStayCation.NoActionBar"/>
        <activity android:name=".HomeActivity" />
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.FYPStayCation.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".RegisterUser"
            android:theme="@style/Theme.FYPStayCation.NoActionBar"/>
        <activity android:name=".ProfileActivity" />
    </application>

</manifest>